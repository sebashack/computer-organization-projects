/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    Nand(a=notA, b=b, out=out0);
    Nand(a=a, b=notB, out=out1);
    Nand(a=out0, b=out1, out=out);
}